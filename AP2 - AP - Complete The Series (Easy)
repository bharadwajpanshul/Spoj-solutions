#include <iostream>
using namespace std;
int main() {
int a;
cin>> a;
while(a--)
{
	long long int c,c3,sum,d,n,a; // c = 3rd term, c3= 3rd last term //
	cin >> c >> c3 >> sum;
	n = (2*sum)/(c+c3);
	d = (c3-c)/(n-5);
	a = c-(2*d);
	cout<<n<<endl;
	for(int i=0;i<n;i++)
	{
		int p = a+(i*d);
		cout<<p<<"";
		cout<<endl;
	}
	
}
	return 0;
}.   // time complexity. = O(n^2) //
